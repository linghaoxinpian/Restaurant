<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<web-app version="2.4" 
    xmlns="http://Java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-app_2_4.xsd">
  <servlet>
    <servlet-name>FoodTypeServlet</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    
    <servlet-class>servlet.FoodTypeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DiningTableServlet</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    
    <servlet-class>servlet.DiningTableServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BaseServlet</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    
    <servlet-class>servlet.BaseServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    
    <servlet-class>servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FoodServlet</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>servlet.FoodServlet</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>FoodTypeServlet</servlet-name>
    <url-pattern>/FoodType</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DiningTableServlet</servlet-name>
    <url-pattern>/DiningTable</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BaseServlet</servlet-name>
    <url-pattern>/Base</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/Index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FoodServlet</servlet-name>
    <url-pattern>/Food</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>Index</welcome-file>
  </welcome-file-list>
</web-app>